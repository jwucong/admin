{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react"
  ],
  "plugins": [
    "react-hot-loader/babel",
    "react-require",
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-syntax-dynamic-import",
    [
      "babel-plugin-import",
      {
        "libraryName": "@material-ui",
        "libraryDirectory": "core"
      }
    ]
  ]
}
